<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5888A6"
    tools:context=".crearCuenta">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="124dp"
        android:layout_marginTop="160dp"
        android:text="CREA UNA CUENTA"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edtNombre"
        android:layout_width="285dp"
        android:layout_height="52dp"
        android:layout_marginTop="224dp"
        android:background="@drawable/bord_new"
        android:backgroundTint="#FFFFFF"
        android:drawableTint="#000000"
        android:ems="10"
        android:foregroundTint="#0F0E0E"
        android:hint="Nombre"
        android:inputType="text"
        android:shadowColor="#141414"
        android:textAlignment="center"
        android:textColor="#070707"
        android:textColorHighlight="#000000"
        android:textColorHint="#545151"
        android:textColorLink="#0F0E0E"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edtApellidos"
        android:layout_width="285dp"
        android:layout_height="52dp"
        android:layout_marginTop="296dp"
        android:background="@drawable/bord_new"
        android:backgroundTint="#FFFFFF"
        android:drawableTint="#000000"
        android:ems="10"
        android:foregroundTint="#0F0E0E"
        android:hint="Apellidos"
        android:inputType="text"
        android:shadowColor="#141414"
        android:textAlignment="center"
        android:textColor="#070707"
        android:textColorHighlight="#000000"
        android:textColorHint="#545151"
        android:textColorLink="#0F0E0E"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/etteEmail1"
        android:layout_width="285dp"
        android:layout_height="52dp"
        android:layout_marginTop="368dp"
        android:background="@drawable/bord_new"
        android:backgroundTint="#FFFFFF"
        android:drawableTint="#000000"
        android:ems="10"
        android:foregroundTint="#0F0E0E"
        android:hint="Correo Electronico"
        android:inputType="textEmailAddress"
        android:shadowColor="#141414"
        android:textAlignment="center"
        android:textColor="#070707"
        android:textColorHighlight="#000000"
        android:textColorHint="#545151"
        android:textColorLink="#0F0E0E"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/ettpPassword"
        android:layout_width="281dp"
        android:layout_height="54dp"
        android:layout_marginTop="436dp"
        android:background="@drawable/bord_new"
        android:ems="10"
        android:hint="ContraseÃ±a"
        android:inputType="textPassword"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHighlight="#0D0D0D"
        android:textColorHint="#545151"
        android:textColorLink="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnSuscribirse"
        android:layout_width="164dp"
        android:layout_height="68dp"
        android:backgroundTint="#FFFFFF"
        android:text="Suscribirse"
        android:textColor="#545151"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.799" />

    <Button
        android:id="@+id/btnAtrasC"
        android:layout_width="61dp"
        android:layout_height="62dp"
        android:backgroundTint="#5888A6"
        android:rotation="-92"
        android:shadowColor="#4E6879"
        android:textColor="#4E6879"
        android:textColorHighlight="#4E6879"
        android:textColorHint="#4E6879"
        android:textColorLink="#4E6879"
        app:icon="@android:drawable/arrow_up_float"
        app:iconTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.011"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.044"
        app:rippleColor="#5888A6"
        app:strokeColor="#5888A6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="152dp"
        android:layout_marginTop="92dp"
        android:text="Suscribirse"
        android:textColor="#FFFFFF"
        android:textSize="26sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="44dp"
        android:text="REGRESAR"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnSingIn"
        android:layout_width="119dp"
        android:layout_height="50dp"
        android:backgroundTint="#5888A6"
        android:text="Sing In"
        android:textColor="#F8F5F5"
        android:textColorHighlight="#FBFBFB"
        android:textColorHint="#FBFBFB"
        android:textColorLink="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.893"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.932" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="648dp"
        android:text="Al ready Have a Account?"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>